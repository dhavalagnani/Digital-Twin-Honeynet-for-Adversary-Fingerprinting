# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/cowrie_monitor.log

# Detection Thresholds (optional - can be modified in code)
BRUTE_FORCE_THRESHOLD=5
BRUTE_FORCE_WINDOW=60 